# -*- coding: utf-8 -*-
import re
import sys
from pydocstringformatter import run_docstring_formatter
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(run_docstring_formatter())
